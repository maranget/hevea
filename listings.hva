\def\lst@bgcolor@in@temp{%
   \let\old@color\color%
   \renewcommand{\color}[2][!*!]{\@getstylecolor[##1]{##2}}%
   \prim@def\lst@temp@bgcolor{background-color:\lst@bgcolor;}%
   \let\color\old@color}
\def\lst@rulecolor@in@temp{%
   \let\old@color\color%
   \renewcommand{\color}[2][!*!]{\@getstylecolor[##1]{##2}}%
   \prim@def\lst@temp@rulecolor{border-color:\lst@rulecolor;}%
   \let\color\old@color}
