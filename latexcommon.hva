%\typeout{Defined text@seen@\@anchorval{\value{onemark}}}%
\@notetextstyle{\@noteref{note}{text}
  {\@anchorval{\value{onemark}}}
  {\@fnmarktext{\value{onemark}}}}\else
\@notetextstyle{\@notepointer{note}
  {\@anchorval{\value{onemark}}}
  {\@fnmarktext{\value{onemark}}}}\fi
}
