\newcommand{\marginpar}[2][]{%
  \def\hva@mtemp{#1}%
  \ifmarginright
    \@print{ >>}#2\@print{>> }%
  \else
    \@print{ <<}\ifx\hva@mtemp\@empty #2\else #1\fi\@print{<< }%
  \fi
}
